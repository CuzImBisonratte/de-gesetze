                        "enbez": [
                            "§ 5"
                        ],
                        "titel": [
                            {
                                "_": "Inkrafttreten",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Diese Verordnung tritt am ersten Tage des auf die Verkündung folgenden Kalendermonats in Kraft."
                                        ]
                                    }
                                ]
                            }
