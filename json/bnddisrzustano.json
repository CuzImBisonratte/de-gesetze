                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "Nach § 33 Abs. 5, § 34 Abs. 2 Satz 2, § 42 Abs. 1 Satz 2 und § 84 Satz 2 des Bundesdisziplinargesetzes vom 9. Juli 2001 (BGBl. I S. 1510) wird der Präsidentin oder dem Präsidenten des Bundesnachrichtendienstes übertragen.",
                                                "DL": [
                                                    {
                                                        "$": {
                                                            "Font": "normal",
                                                            "Type": "arabic"
                                                        },
                                                        "DT": [
                                                            "1.",
                                                            "2.",
                                                            "3.",
                                                            "4."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Zuständigkeit, nach § 42 Abs. 1 des Bundesdisziplinargesetzes den Widerspruchsbescheid zu erlassen, soweit sie oder er zum Erlass der angefochtenen Entscheidung zuständig war,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Befugnis, nach § 33 Abs. 1 in Verbindung mit Abs. 3 Nr. 1 des Bundesdisziplinargesetzes die Kürzung der Dienstbezüge bis zum Höchstmaß festzusetzen,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Befugnis, nach § 34 Abs. 1 in Verbindung mit Abs. 2 Satz 1 des Bundesdisziplinargesetzes Disziplinarklage zu erheben und",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Befugnis, nach § 84 Satz 1 des Bundesdisziplinargesetzes gegenüber Ruhestandsbeamtinnen und Ruhestandsbeamten die Disziplinarbefugnisse auszuüben,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            },
                                            {
                                                "SP": [
                                                    "Der Chef des Bundeskanzleramtes"
                                                ]
                                            }
                                        ]
                                    }
