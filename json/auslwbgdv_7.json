                ]
            },
            {
                "$": {
                    "builddate": "20120625150940",
                    "doknr": "BJNR015220953BJNE000200306"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "AuslWBGDV 7"
                        ],
                        "enbez": [
                            "§ 1"
                        ],
                        "titel": [
                            {
                                "_": "Festsetzung eines späteren Stichtags",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "Der Stichtag für die im Verzeichnis der Auslandsbonds (Anlage zu § 1 Abs. 1 des Gesetzes,  aufgeführten Arten von Auslandsbonds wird auf den 1. September 1953 festgesetzt.",
                                                "I": [
                                                    "ergänzt durch § 1 der Ersten Durchführungsverordnung vom 21. Februar 1953 - Bundesgesetzbl. I S. 31 -)"
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20120625150940",
