                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "Gemäß § 9 der Geschäftsordnung der Bundesregierung ordne ich mit sofortiger Wirkung an: ",
                                                "DL": [
                                                    {
                                                        "$": {
                                                            "Font": "normal",
                                                            "Type": "arabic"
                                                        },
                                                        "DT": [
                                                            "I.",
                                                            "II.",
                                                            "III.",
                                                            "IV."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "Das Bundesministerium für Ernährung, Landwirtschaft und Forsten wird zu einem Bundesministerium für Verbraucherschutz, Ernährung und Landwirtschaft umgebildet.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "Dazu werden ihm übertragen",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        },
                                                                        "DL": [
                                                                            {
                                                                                "$": {
                                                                                    "Font": "normal",
                                                                                    "Type": "arabic"
                                                                                },
                                                                                "DT": [
                                                                                    "-",
                                                                                    "-"
                                                                                ],
                                                                                "DD": [
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "aus dem Geschäftsbereich des Bundesministeriums für Gesundheit die Zuständigkeiten für Verbraucherschutz (Abteilung 4) mit der Unterabteilung 41 (Lebensmittel und Bedarfsgegenstände) und der Unterabteilung 42 (Veterinärmedizin);",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            },
                                                                                            {
                                                                                                "_": "ausgenommen sind die Zuständigkeiten",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                },
                                                                                                "DL": [
                                                                                                    {
                                                                                                        "$": {
                                                                                                            "Font": "normal",
                                                                                                            "Type": "arabic"
                                                                                                        },
                                                                                                        "DT": [
                                                                                                            "-",
                                                                                                            "-"
                                                                                                        ],
                                                                                                        "DD": [
                                                                                                            {
                                                                                                                "$": {
                                                                                                                    "Font": "normal"
                                                                                                                },
                                                                                                                "LA": [
                                                                                                                    {
                                                                                                                        "_": "für Chemikaliensicherheit (Referat 417),",
                                                                                                                        "$": {
                                                                                                                            "Size": "normal"
                                                                                                                        }
                                                                                                                    }
                                                                                                                ]
                                                                                                            },
                                                                                                            {
                                                                                                                "$": {
                                                                                                                    "Font": "normal"
                                                                                                                },
                                                                                                                "LA": [
                                                                                                                    {
                                                                                                                        "_": "für die Zulassung von Tierarzneimitteln und das Berufsrecht der Veterinäre und sonstigen veterinärmedizinischen Berufe (Referat 425);",
                                                                                                                        "$": {
                                                                                                                            "Size": "normal"
                                                                                                                        }
                                                                                                                    }
                                                                                                                ]
                                                                                                            }
                                                                                                        ]
                                                                                                    }
                                                                                                ]
                                                                                            }
                                                                                        ]
                                                                                    },
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "aus dem Geschäftsbereich des Bundesministeriums für Wirtschaft und Technologie die Zuständigkeit für Verbraucherpolitik einschließlich Stiftung Warentest (Referat I B 4).",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            }
                                                                                        ]
                                                                                    }
                                                                                ]
                                                                            }
                                                                        ]
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "Das Bundesinstitut für gesundheitlichen Verbraucherschutz und Veterinärmedizin (BgVV) wird in den Geschäftsbereich des Bundesministeriums für Verbraucherschutz, Ernährung und Landwirtschaft verlagert.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "Die weiteren Einzelheiten des Übergangs werden zwischen den beteiligten Bundesministerien in Abstimmung mit dem Chef des Bundeskanzleramtes geregelt.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            },
                                            "Der Bundeskanzler"
                                        ]
                                    }
