                            "MusikfachhPrErprV"
                        ],
                        "enbez": [
                            "§ 6"
                        ],
                        "titel": [
                            {
                                "_": "Bestehende Berufsausbildungsverhältnisse",
                                "$": {
                                    "format": "XML"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "(1) (weggefallen)",
                                            "(2) Auf Berufsausbildungsverhältnisse, die bis zum Ablauf des 31. Juli 2015 begonnen werden, sind die Vorschriften dieser Verordnung weiter anzuwenden."
                                        ]
                                    }
                                ]
                            }
