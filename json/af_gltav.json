                        "enbez": [
                            "§ 3"
                        ],
                        "titel": [
                            {
                                "_": "Berlin-Klausel",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Diese Verordnung gilt nach § 14 des Dritten Überleitungsgesetzes vom 4. Januar 1952 (Bundesgesetzblatt I S. 1) in Verbindung mit § 42 des Ausbildungsförderungsgesetzes auch im Land Berlin."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
