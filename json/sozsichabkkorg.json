                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "(1) Dieses Gesetz tritt am Tage nach seiner Verkündung in Kraft.",
                                            "(2) Der Tag, an dem das Abkommen nach seinem Artikel 24 Abs. 2 in Kraft tritt, ist im Bundesgesetzblatt bekannt zu geben."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
