                                                            "1.",
                                                            "2.",
                                                            "3.",
                                                            "4.",
                                                            "5.",
                                                            "6.",
                                                            "7.",
                                                            "8.",
                                                            "9."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    "der Bundesanstalt für Landwirtschaft und Ernährung,"
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    "dem Bundessortenamt,"
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    "dem Bundesamt für Verbraucherschutz und Lebensmittelsicherheit,"
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
