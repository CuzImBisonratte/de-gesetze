            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000100314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 1"
                        ],
                        "titel": [
                            {
                                "_": "Höhengleiche Kreuzungen",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "(1) Zur Kreuzungsanlage im Sinne des § 13 Abs. 1 FStrG, die der Baulastträger der Bundesfernstraße zu unterhalten hat, gehören  ",
                                                "DL": [
                                                    {
                                                        "$": {
                                                            "Font": "normal",
                                                            "Type": "arabic"
                                                        },
                                                        "DT": [
                                                            "1.",
                                                            "2."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "von der die Bundesfernstraße kreuzenden Straße vom Anfang ihrer Eckausrundungen an",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        },
                                                                        "DL": [
                                                                            {
                                                                                "$": {
                                                                                    "Font": "normal",
                                                                                    "Type": "arabic"
                                                                                },
                                                                                "DT": [
                                                                                    "-",
                                                                                    "-",
                                                                                    "-",
                                                                                    "-"
                                                                                ],
                                                                                "DD": [
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "die befestigten Fahrstreifen einschließlich Trenn-, Seiten- und Randstreifen,",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            }
                                                                                        ]
                                                                                    },
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "die Verkehrszeichen und -einrichtungen sowie Verkehrsanlagen aller Art, insbesondere Verkehrsinseln,",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            }
                                                                                        ]
                                                                                    },
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "die Gehwege außerhalb der Ortsdurchfahrten und die Radwege, soweit diese Wege mit der kreuzenden Straße in Zusammenhang stehen und mit dieser gleichlaufen,",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            }
                                                                                        ]
                                                                                    },
                                                                                    {
                                                                                        "$": {
                                                                                            "Font": "normal"
                                                                                        },
                                                                                        "LA": [
                                                                                            {
                                                                                                "_": "die Durchlässe, Dämme, Gräben, Böschungen und Stützmauern,",
                                                                                                "$": {
                                                                                                    "Size": "normal"
                                                                                                }
                                                                                            }
                                                                                        ]
                                                                                    }
                                                                                ]
                                                                            }
                                                                        ]
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die durch die Kreuzung bedingten Lichtzeichenanlagen.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            },
                                            "(2) Eine Eckausrundung beginnt an der Stelle, an der der erste Radius die Ecken der Straßenränder von der Bundesfernstraße und der kreuzenden Straße abzurunden beginnt.",
                                            "(3) Sichtfelder gehören zur kreuzenden Straße."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000200314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 2"
                        ],
                        "titel": [
                            {
                                "_": "Über- und Unterführungen",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "(1) Zum Kreuzungsbauwerk im Sinne des § 13 Abs. 2 des Bundesfernstraßengesetzes gehören  ",
                                                "DL": [
                                                    {
                                                        "$": {
                                                            "Font": "normal",
                                                            "Type": "arabic"
                                                        },
                                                        "DT": [
                                                            "1.",
                                                            "2.",
                                                            "3."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Widerlager mit Flügelmauern,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "die Pfeiler,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "der Überbau mit Geländern, Brüstungen und Auffangvorrichtungen, jedoch mit Ausnahme der Straßendecke, der Entwässerungsrinnen und Einläufe und, soweit sie nicht durch die Konstruktion der Brücke bedingt sind, der Verkehrszeichen und -einrichtungen sowie Verkehrsanlagen aller Art.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            },
                                            "(2) Die nicht zum Kreuzungsbauwerk rechnenden Teile des Überbaus (Abs. 1 Nr. 3) gehören zu der Straße, in deren Verlauf sie liegen.",
                                            "(3) Verbindungsarme zwischen der Bundesfernstraße und der kreuzenden Straße gehören zur Bundesfernstraße. Die Verbindungsarme enden am äußeren Fahrbahnrand der kreuzenden Straße. Sind Abbiege- oder Einfädelstreifen auf der kreuzenden Straße vorhanden, so enden die Verbindungsarme am Anfang der Eckausrundungen der kreuzenden Straße. Bei höhenungleichen Einmündungen beginnen die Verbindungsarme an der ersten Aufweitung der einmündenden Straße. Lichtzeichenanlagen und Verkehrsinseln an der Einmündung des Verbindungsarms gehören zur Bundesfernstraße."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000300314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 3"
                        ],
                        "titel": [
                            {
                                "_": "Sonstige Teile der Kreuzungsanlage",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Die in den §§ 1 und 2 nicht erfaßten Teile der Bundesfernstraße und der kreuzenden Straße gehören zu der Straße, der sie unmittelbar dienen."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000400314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 4"
                        ],
                        "titel": [
                            {
                                "_": "Kreuzungen zwischen Bundesfernstraßen",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Auf Kreuzungen zwischen Bundesfernstraßen, von denen die eine vom Bund, die andere von einem Dritten unterhalten wird, ist diese Verordnung mit der Maßgabe anzuwenden, daß die vom Dritten unterhaltene Bundesfernstraße als kreuzende Straße gilt."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000500314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 5"
                        ],
                        "titel": [
                            {
                                "_": "Einmündungen",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Die vorstehenden Vorschriften gelten auch für Einmündungen öffentlicher Straßen in Bundesfernstraßen."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
                "$": {
                    "builddate": "20130405130304",
                    "doknr": "BJNR029850975BJNE000600314"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "FStrKrV"
                        ],
                        "enbez": [
                            "§ 6"
                        ],
                        "titel": [
                            {
                                "_": "Berlin-Klausel",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Diese Verordnung gilt nach § 14 des Dritten Überleitungsgesetzes vom 4. Januar 1952 (Bundesgesetzblatt I S. 1) in Verbindung mit § 26 des Bundesfernstraßengesetzes auch im Land Berlin."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}