                            "§ 3"
                        ],
                        "titel": [
                            {
                                "_": "Rechnungsprüfung",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Die Haushalts- und Wirtschaftsführung der Stiftung unterliegt der Prüfung durch den Bundesrechnungshof."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            {
