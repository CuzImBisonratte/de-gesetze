                        "langue": [
                            "Gesetz über eine einmalige Entschädigung an die Heimkehrer aus dem Beitrittsgebiet"
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "fussnoten": [
                            {
                                "Content": [
                                    {
