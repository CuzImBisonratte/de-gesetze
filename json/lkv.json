                                                                "LA": [
                                                                    {
                                                                        "_": "Lebensmittel, deren Kennzeichnung mit der Angabe des Loses",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        },
                                                                        "DL": [
                                                                            {
                                                                                "$": {
