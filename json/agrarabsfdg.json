                        ],
                        "enbez": [
                            "§ 6"
                        ],
                        "titel": [
                            {
                                "_": "Inkrafttreten",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Dieses Gesetz tritt am Tage nach der Verkündung in Kraft."
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
