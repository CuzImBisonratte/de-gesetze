                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "(1) Dieses Gesetz tritt am Tag nach der Verkündung in Kraft.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "(2) Der Tag, an dem das Abkommen nach seinem Artikel 26 Absatz 2 in Kraft tritt, ist im Bundesgesetzblatt bekannt zu geben.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
