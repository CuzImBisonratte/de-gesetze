                                                                "LA": [
                                                                    {
                                                                        "_": "des § 255b Abs. 1 des Sechsten Buches Sozialgesetzbuch (Artikel 1 des Gesetzes vom 25. Juli 1991, BGBl. I S. 1606),",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "des § 558 Abs. 3 und des § 579 Abs. 2 der Reichsversicherungsordnung, zuletzt geändert durch Artikel 6 Nr. 2 und 7 des Gesetzes vom 18. Dezember 1989 (BGBl. I S. 2261),",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "der §§ 1151, 1153 der Reichsversicherungsordnung, die durch Artikel 8 Nr. 14 des Gesetzes vom 25. Juli 1991 (BGBl. I S. 1606) eingefügt worden sind,",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
