                            "§ 2"
                        ],
                        "titel": [
                            {
                                "_": "Herkunftsgebiet",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            "Das Solinger Industriegebiet umfaßt das Gebiet der kreisfreien Stadt Solingen und das Gebiet der im Kreis Mettmann gelegenen Stadt Haan."
                                        ]
                                    }
                                ]
                            }
                        ]
