                    "builddate": "20130405133007",
                    "doknr": "BJNR383300994BJNE000200000"
                },
                "metadaten": [
                    {
                        "jurabk": [
                            "SolingenV"
                        ],
                        "enbez": [
                            "§ 1"
                        ],
                        "titel": [
                            {
                                "_": "Grundsatz",
                                "$": {
                                    "format": "parat"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
                    {
                        "text": [
                            {
                                "$": {
                                    "format": "XML"
                                },
                                "Content": [
                                    {
                                        "P": [
                                            {
                                                "_": "Der Name Solingen darf im geschäftlichen Verkehr nur für solche Schneidwaren benutzt werden, die ",
                                                "DL": [
                                                    {
                                                        "$": {
                                                            "Font": "normal",
                                                            "Type": "arabic"
                                                        },
                                                        "DT": [
                                                            "1.",
                                                            "2."
                                                        ],
                                                        "DD": [
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "in allen wesentlichen Herstellungsstufen innerhalb des Solinger Industriegebiets bearbeitet und fertiggestellt worden sind und",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$": {
                                                                    "Font": "normal"
                                                                },
                                                                "LA": [
                                                                    {
                                                                        "_": "nach Rohstoff und Bearbeitung geeignet sind, ihren arteigenen Verwendungszweck zu erfüllen.",
                                                                        "$": {
                                                                            "Size": "normal"
                                                                        }
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
