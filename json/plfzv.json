                        "enbez": [
                            "§ 1"
                        ],
                        "titel": [
                            {
                                "_": "Durchführung der Planfeststellung durch die Bundesnetzagentur",
                                "$": {
                                    "format": "XML"
                                }
                            }
                        ]
                    }
                ],
                "textdaten": [
